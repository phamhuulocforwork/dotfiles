{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#6272a4",
          "foreground": "#f8f8f2",
          "leading_diamond": "\u250c",
          "properties": {
            "folder_icon": "\uf115",
            "home_icon": "\ueb06",
            "style": "folder"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\u2500",
          "type": "path"
        },
        {
          "background": "#bd93f9",
          "foreground": "#f8f8f2",
          "properties": {
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "\uf444",
            "branch_gone": "\ufb2b",
            "branch_ahead_icon": "\u2191",
            "branch_behind_icon": "\u2193",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (not .Working.Changed) }}{{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\u2500",
          "type": "git"
        },
        {
          "background": "#ffb86c",
          "foreground": "#f8f8f2",
          "properties": {
            "fetch_version": true,
            "display_mode": "context",
            "version_url_template": "https://nodejs.org/dist/v{{ .Version }}/docs/api/"
          },
          "style": "diamond",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "trailing_diamond": "\u2500",
          "type": "node"
        },
        {
          "background": "#ff5555",
          "foreground": "#f8f8f2",
          "properties": {
            "fetch_version": true,
            "display_mode": "context"
          },
          "style": "diamond",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\u2500",
          "type": "python"
        },
        {
          "background": "#50fa7b",
          "foreground": "#f8f8f2",
          "properties": {
            "display_mode": "context",
            "fetch_version": true
          },
          "style": "diamond",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "trailing_diamond": "\u2518",
          "type": "shell"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "version": 2
}
